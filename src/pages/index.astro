---
import Navbar from "../components/Navbar.astro";
import Hero from "../components/Hero.astro";
import PartnersSection from "../components/PartnersSection.astro";
import UnifiedPlatformSection from "../components/UnifiedPlatformSection.astro";
import GraphicSection from "../components/GraphicSection.astro";
import IntegrationsSection from "../components/IntegrationsSection.astro";
import WhySwipeSection from "../components/WhySwipeSection.astro";
import GlobalSection from "../components/GlobalSection.astro";
import VirtualEventsSection from "../components/VirtualEventsSection.astro";
import GettingStartedSection from "../components/GettingStartedSection.astro";
import Footer from "../components/Footer.astro";
import GradientCanvas from "../components/GradientCanvas.astro";
---

<!doctype html>
<html lang="en">
<head>
  <meta charset="UTF-8"/>
  <link href="/assets/logo-white.svg" rel="icon" type="image/svg+xml"/>
  <meta content="width=device-width, initial-scale=1.0" name="viewport"/>
  <title>Swipe Project</title>
  <link href="/styles.css" rel="stylesheet"/>
  <link href="https://fonts.googleapis.com" rel="preconnect">
  <link crossorigin href="https://fonts.gstatic.com" rel="preconnect">
  <link
    href="https://fonts.googleapis.com/css2?family=Anek+Telugu:wght@300;400;500;600;700&family=Open+Sans:wght@300;400;600;700&family=Roboto:wght@300;400;500;700&display=swap"
    rel="stylesheet"
  >
</head>
<body>
<Navbar/>

<main>
  <GradientCanvas/>
  <Hero/>
  <PartnersSection/>
  <UnifiedPlatformSection/>
  <GraphicSection/>
  <IntegrationsSection/>
  <WhySwipeSection/>
  <GlobalSection/>
  <VirtualEventsSection/>
  <GettingStartedSection/>
  <Footer/>
</main>
</body>
</html>
